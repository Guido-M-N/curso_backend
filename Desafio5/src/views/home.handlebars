<div>
    <h1>Product catalogue</h1>
    <table>
        <thead>
            <tr>
                <th>Title</th>
                <th>Description</th>
                <th>Price</th>
            </tr>
        </thead>
        <tbody>
            {{#each products}}
            <tr>
                <td>{{title}}</td>
                <td>{{price}}</td>
                <td>{{description}}</td>
                {{!-- <td><img src="{{this.thumbnail}}" alt="not found" width="50" height="50"></td> --}}
            </tr>
            {{/each}}
        </tbody>
    </table>
</div>